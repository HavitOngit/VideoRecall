{
  "name": "VideoRecall",
  "description": "undo/redo functionality for video playback",
  "version": "1.0",
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "videorecall@havit",
      "strict_min_version": "121.0"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
